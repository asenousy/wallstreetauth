 <div id="container"></div>
 {{> header loggedIn=loggedIn }}

 <div id="body">
 <div class="jumbotron">
<h1>Wall Street Docs</h1>
</div>

{{#if loggedIn}}
    <h3>Welcome Member you are currently logged in</h3>
{{else}}
    <h3>Welcome Wall Street Docs</h3>
{{/if}}

<hr>
{{#unless loggedIn}}

<a id="login" href="/auth/wallstreet" class="btn btn-primary">Log using Wallstreet Account</a>

{{#if pleaseLogin}}
    <script>
        $(document).ready(function () {
            $("#login").twist(360)
        })
    </script>
{{/if}}

{{/unless}}

</div>

  {{> footer}}

</div>